{
  "name": "svelte-app",
  "version": "1.1.0",
  "scripts": {
    "brotli:dist": "node br.js",
    "build": "routify -b && vite build && npm run brotli:dist && spank",
    "dev": "routify -c dev-vite ",
    "dev-vite": "vite"
  },
  "devDependencies": {
    "@roxi/routify": "^2.14.0",
    "@svitejs/vite-plugin-svelte": "^0.10.0",
    "@types/html-minifier": "^4.0.0",
    "brotli": "^1.3.2",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "mdsvex": "^0.8.9",
    "remark-slug": "^6.0.0",
    "routify-plugin-frontmatter": "^1.0.1",
    "spank": "^1.7.0",
    "svelte": "^3.35.0",
    "svelte-preprocess": "^4.6.9",
    "vite-monorepo": "github:GrygrFlzr/vite"
  },
  "routify": {
    "extensions": [
      "md",
      "svx",
      "svelte"
    ],
    "plugins": {
      "routify-plugin-frontmatter": {}
    }
  }
}
